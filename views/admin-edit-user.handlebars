
  <div class="card">
    <div class="card-header d-flex justify-content-between" >
      <div class="containerfluid align-self-center">
    <h3>{{this.name}} Profile</h3>
    </div>
    <div class="containerfluid">
    <button href="#" class="btn btn-danger btn-lg ">Delet user</button>
    </div>
    </div>
    <div class="card-body">
      <table class="table table-user-information">
        <tbody>
          <tr>
            <td>Role:</td>
            {{#if db_User.[0].isadmin}}
              <td>Admin</td>
            {{else}}
              <td>User</td>
            {{/if}}
          </tr>
          <tr>
            <td>Name:</td>
            <td>{{db_User.[0].name}}</td>
          </tr>
          <tr>
            <td>Username:</td>
            <td>{{db_User.[0].username}}</td>
          </tr>
          <tr>
            <td>Register date:</td>
            <td>{{db_User.[0].registerdate}} </td>
          </tr>
          <tr>
            <td>Email</td>
            <td><a href="mailto:{{db_User.[0].email}}">{{db_User.[0].email}}</a></td>
          </tr>
          <tr>
            <td>Number of projects:</td>
            <td>{{db_Data.length}}</td>
          </tr>
        </tbody>
      </table>
      <a href="/admin/admin-edit-profile{{db_User.[0].id}}" class="btn btn-primary btn-lg  btn-block">Edit {{this.name}} profile</a>
    </div>
  </div>


  <div class="card">
    <h3 class="card-header">{{this.name}} Project</h3>
    <div class="card-body">

  <table class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Name</th>
        <th scope="col">Creation</th>
        <th scope="col">Lastedit</th>
        <th scope="col">Deleted</th>
        <th scope="col"></th>

      </tr>
    </thead>
    {{#each db_Data}}
    <tbody>
      <tr>
        <th scope="row">1</th>
        <td>{{this.name}}</td>
        <td>{{this.creationdate}}</td>
        <td>{{this.lastedit}}</td>
        {{#if this.deleted}}
        <td>Yes</td>
        {{else}}
        <td>No</td>
        {{/if}}
        <td><a type="button" href="/exports/{{this._id}}" class="btn btn-primary">Edit</a></td>
      </tr>
    </tbody>
    {{/each}}
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">Username</th>
      <th scope="col">Email</th>
      <th scope="col">Rank</th>
      <th scope="col"></th>

    </tr>
  </table>
  </div>
